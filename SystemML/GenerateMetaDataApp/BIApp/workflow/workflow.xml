<workflow-app name="GenerateMetadata" xmlns="uri:oozie:workflow:0.1">
    <start to="action1"/>
    <action name="action1">
        <jaql xmlns="uri:oozie:jaql-action:0.1">
            <job-tracker>${jobTracker}</job-tracker>
            <name-node>${nameNode}</name-node>
            <jaql-path>custom_modules</jaql-path>
            <script>jaql/run_GenerateMetadata.jaql</script>
            <eval>
				Input_input_PARAM='${input}';
				Input_inputDelimiter_PARAM='${wf:conf("inputDelimiter")}';
				Input_inputMetadata_PARAM='${wf:conf("inputMetadata")}';
    	    </eval>
        </jaql>
        <ok to="end"/>
        <error to="fail"/>
    </action>
    <kill name="fail">
        <message>error message[${wf:errorMessage(wf:lastErrorNode())}]</message>
    </kill>
    <end name="end"/>
</workflow-app>