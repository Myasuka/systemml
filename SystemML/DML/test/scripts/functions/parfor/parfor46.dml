numC = 5;
numF = 10;

A = Rand(rows=numC, cols=numF, min=0.0, max=1.0, sparsity=1.0)
B = matrix(0, rows=numC, cols=numF)  #classFeatureCumulativeDensityFunction
C = matrix(0, rows=numC, cols=numF)  #tempCumulative

parfor(i in 1:numC ) 
{
	A[i,] = A[i,]/sum(A[i,]);
  C[i,1] = A[i,1];
	
  for (j in 2:numF) 
  {
		 C[i,j] = C[i,j-1] + A[i,j];
  }
  
  B[i,] = C[i,];
}	





