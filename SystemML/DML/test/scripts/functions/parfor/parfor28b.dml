A = matrix(0,rows=20,cols=20);
B = Rand(rows=20,cols=20);

parfor( i in 1:20 )
{ 
   k = 0;
   while( k < 10 )
   {
     parfor( j in 1:20 )
     {
         A[i,j] = B[i,j]+(i+j);
     }
     
     k = k + 1;
   }
}

