dummy = matrix(1, rows=1,cols=1);

parfor(i in 1:a){
	for(j in 2:b){
		A[i,j] = A[i,j-1] + A[i,j]
	}
}

parfor(i in 1:c){
	  parfor(j in 1:d){
    	for(k in 1:b){
    		B[i,j] = dummy*(i*j);
    	}
    }
}   


