seq = externalFunction(Matrix[Double] A, Double sup, Double max_level) 
return (Matrix[Double] B, Matrix[Double] C) 
implemented in (classname="dml.packagesupport.SequenceMinerWrapper",execlocation="master");
 
minsup = 0.004;
max_level=5.0;


M = readMM( "$$indir$$M", rows=$$rows$$, cols=$$cols$$, format="text");

[A,B] = seq(M,minsup,max_level);


writeMM(A, "$$outdir$$fseq", format="text");
writeMM(B, "$$outdir$$sup", format="text");
