order = externalFunction(Matrix[Double] A, Integer col, Boolean desc) return (Matrix[Double] B) 
			    implemented in (classname="com.ibm.bi.dml.packagesupport.OrderWrapper",exectype="mem")
 
A = read( $1, rows=$2, cols=$3);  
B1 = order(A, $4, true);

write(B1, $5);

