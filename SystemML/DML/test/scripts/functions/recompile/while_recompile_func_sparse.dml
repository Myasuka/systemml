foo1 = function (Matrix[Double] X)
    return (Matrix[Double] Y)
{  
   V = X;
   print(sum(V)); 
   i = 1;
   while( i <= 1 )
   {
      print(sum(V)); 
      V = foo2(V,i);
      i = i+1;
   }
   Y = V;  
}

foo2 = function (Matrix[Double] X, Integer i)
    return (Matrix[Double] Y)
{  
   V = X;
   j = 1; 
   while( j <= 1 )
   {
      V[i,j] = $2;
      j = j+1; 
   }    
   Y = V;   
}

V = read($1);
V = foo1( V );
write(V, $3);        