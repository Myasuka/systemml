foo1 = function (Matrix[Double] X)
    return (Matrix[Double] Y)
{  
   V = X;
   print(sum(V)); 
   if( 1==1 ){
      V[1,1] = $2;
   }
   else {
      print(sum(V));
   }   
   Y = V;  
}

foo2 = function (Matrix[Double] X)
    return (Matrix[Double] Y)
{  
   V = X;     
   if( 1==1 ){
      print(sum(V));
   } 
   Y = V;   
}

V = read($1);
V = foo1(V);
V = foo2(V);
write(V, $3);         