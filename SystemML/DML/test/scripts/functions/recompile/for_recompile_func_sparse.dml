foo1 = function (Matrix[Double] X)
    return (Matrix[Double] Y)
{  
   V = X;
   print(sum(V)); 
   for( i in 1:1 )
   {
      print(sum(V));      
      V = foo2(V,i);
   }   
   Y = V;  
}

foo2 = function (Matrix[Double] X, Integer i)
    return (Matrix[Double] Y)
{  
   V = X;     
   for( j in 1:1 )
   {
      V[i,j] = $2; 
   } 
   Y = V;   
}

V = read($1);
V = foo1(V); 
write(V, $3);    




   