from = $1;
to = $2;

# test auto casting constant propagation
for( i in from:to ) {
   print( "Result: "+i );
}

# test auto casting with variables
if( 1==1 ){
   from = 1 + from;
   to = 10 + to;
}
for( i in from:to ) {
   print( "Result: "+i );
}
