# Script for computing bivariate statistics between two ordinal variables
# (with no weights)

#A <- Ordinal
#B <- Ordinal

A = readMM("in/educ.ordinal.data", rows=474, cols=1, format="text");
B = readMM("in/jobcat.ordinal.data", rows=474, cols=1, format="text");

sp = spearman(A,B);

print("Spearman correlation = ", sp);
