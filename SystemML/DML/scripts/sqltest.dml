W = readMM( "DMLmatrixWin", rows=1000000, cols=10, format="text");
H = readMM( "DMLmatrixHin", rows=10, cols=100000, format="text");

V = readMM( "DMLmatrixVin", rows=1000000, cols=100000, format="text");
H = H * ((t(W) %*% V) /  ( (t(W) %*% W) %*% H));
W = W * ((V %*% t(H)) / ( W %*% (H %*% t(H))));
a = 2;
b = max(H);
c = a * b;
W = W * c;

a = 0;
while(a < 10)
{
	a = a + 1;
}

writeMM(H, "tableH");
writeMM(W, "tableW");