# input vector (column matrix)
V = readMM($1, rows=$2, cols=1, rows_per_block=$3, cols_per_block=1, format="binary");

# a vector specifying the desired quantiles
P = readMM($4, rows=$5, cols=1,  format="text");

# median
median = quantile(V, 0.5);

# quantiles
Q = quantile(V, P);

# inter-quartile mean
iqm = interQuartileMean(V);

print("median: ", median);
print("inter-quartile mean: ", iqm);
write(Q, $6, format="text");