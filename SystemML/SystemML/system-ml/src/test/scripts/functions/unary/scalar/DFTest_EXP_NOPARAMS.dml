q = qexp(target=$1); # default rate=1.0
p = pexp(target=q);
pl = pexp(target=q, lowertail=FALSE);

out = matrix(0, rows=3, cols=1);
out[1,1] = q;
out[2,1] = p;
out[3,1] = pl;

write(out, $2);
