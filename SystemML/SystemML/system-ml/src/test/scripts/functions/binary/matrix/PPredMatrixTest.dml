A = read( $1 );
B = read( $2 );

if( $3 == 0 )
{
   C = ppred(A, B, ">")
}
else if( $3 == 1 )
{
   C = ppred(A, B, "<")
}
else if( $3 == 2 )
{  
   C = ppred(A, B, "==")
}
else if( $3 == 3 )
{
   C = ppred(A, B, "!=")
}
else if( $3 == 4 )
{
   C = ppred(A, B, ">=")
}
else if( $3 == 5 )
{
   C = ppred(A, B, "<=")
}

write(C, $4, format="text");