foo = function(Double input) return (Double out)
{
   out = input + input; #will be inlined
}

foo2 = function(Integer input) return (Double out)
{ 
   out = input + input; #will be inlined
}

x = $1;

if( 1!=1 ){
  x = x;
}

ret1 = foo(x);
ret2 = foo2(x);
 
print("Result1: "+ret1);
print("Result2: "+ret2);
