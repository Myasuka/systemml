eigen1 = externalFunction(Matrix[Double] A) 
return (Matrix[Double] E, Matrix[Double] V) 
implemented in (classname="com.ibm.bi.dml.udf.lib.EigenWrapper",exectype="mem")
 
A = read( "$$indir$$A", rows=$$rows$$, cols=$$cols$$, format="text");
[E,V] = eigen1(A);

write(E, "$$outdir$$val", format="text");
write(V, "$$outdir$$vec", format="text");
