foo = function( Matrix[Double] A ) return( Matrix[Double] B )
{
   continue = TRUE;
   iter = 0;
   while( continue )
   {
      iter = iter+1;
      if( iter<10 )
          continue = TRUE;
      else
          continue = FALSE;
   }
   B = A+iter;
}


A = matrix(1, rows=10, cols=10);
R = foo(A);

write(R, $1);