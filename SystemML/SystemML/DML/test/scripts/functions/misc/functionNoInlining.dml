foo = function(Double in) return (Double out)
{
   if( 1==1 ){ #prevent inlining
      out = in + in;
   }
}

foo2 = function(Integer in) return (Double out)
{ 
   if( 1==1 ){ #prevent inlining
      out = in + in;
   }
}

x = $1;

if( 1!=1 ){
  x = 7;
}

ret1 = foo(x);
ret2 = foo2(x);
 
print("Result1: "+ret1);
print("Result2: "+ret2);
