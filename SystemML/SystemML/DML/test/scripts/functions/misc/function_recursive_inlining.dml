   V = foo3(B+1);
}

foo3 = function( Matrix[Double] B ) return (Matrix[Double] V) {
   V = foo4(B+1);
}

foo4 = function( Matrix[Double] B ) return (Matrix[Double] V) {
   V = foo5(B+1);
}

foo5 = function( Matrix[Double] B ) return (Matrix[Double] V) {
