foo = function(Matrix[Double] input) return (Double out) 
{
   if( 1==1 ){} #prevent inlining
  
   out = castAsScalar(input[1,1]);
}

Y = matrix(1, rows=10, cols=10);
X = matrix(7, rows=10, cols=10);
X = foo(X);

print("Result: "+sum(X + Y));
#expected: "Result: 800.0"
