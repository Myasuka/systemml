X = read($1);
y = read($2);
I = matrix(1, ncol(X), 1);
lambda = 0;

if( 1==1 ) {}

A = t(X) %*% X + diag(I)*lambda;
b = t(X) %*% y;
beta = solve(A, b);

write(beta, $3);
