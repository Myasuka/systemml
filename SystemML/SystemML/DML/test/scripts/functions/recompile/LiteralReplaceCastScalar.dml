scalar = $1;

if( 1==1 ){
   A = Rand(rows=10, cols=10, min=1, max=2);
   B = Rand(rows=10, cols=10, min=3, max=4);
   C = Rand(rows=as.scalar(A[1,1]), cols=as.scalar(B[1,1]));
}  

ret1 = sum(C) * as.double(scalar);
ret2 = sum(C) * as.integer(scalar);

if( sum(C)>0 & as.logical(scalar) ){
   ret3 = sum(C) * as.double(scalar);
}   
else {
   ret3 = sum(C);
}   

print("Ret1 = "+ret1);
print("Ret2 = "+ret2);
print("Ret3 = "+ret3);