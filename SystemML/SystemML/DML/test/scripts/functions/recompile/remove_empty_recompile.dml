execFun = function(Matrix[Double] X, Integer type) 
  return (Matrix[Double] R) 
{
   R = X;

   if( type==0 ){
      R = as.matrix( sum(X) );
   }
   if( type==1 ){
      R = round(X);
   }
   if( type==2 ){
      R = t(X); 
   }
   if( type==3 ){
      R = X*(X-1);
   }
   if( type==4 ){
      R = (X-1)*X;
   }
   if( type==5 ){
      R = X+(X-1);
   }
   if( type==6 ){
      R = (X-1)+X;
   }
   if( type==7 ){
      R = X-(X+2);
   }
   if( type==8 ){    
      R = (X+2)-X;
   }
   if( type==9 ){
      R = X%*%(X-1);
   }
   if( type==10 ){
      R = (X-1)%*%X;
   }
   if( type==11 ){
      R = X[1:(nrow(X)-1), 1:(ncol(X)-1)];
   }
   if( type==12 ){
      X[1,] = X[2,];
      R = X;
   }
}

X = read($1);
R = execFun( X, $2 )  
write(R, $3);       