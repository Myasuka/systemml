/*
 * DML script to test LU Factorization
 */

A = read($1);

[P, L, U] = lu(A);

PA = P %*% A;
LU = L %*% U;

diff = sum(PA - LU);
D = matrix(1,1,1);
D = diff*D;

write(D, $2);

