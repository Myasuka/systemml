V = read($1);
print(sum(V)); 
i = 1;
while( i <= 1 )
{
   print(sum(V)); 
   
   j = 1; 
   while( j <= 1 )
   {
      V[i,j] = $2;
      j = j+1; 
   }
   i = i+1;
}  
write(V, $3);        